{
  "name": "react-webpack-one-page-wonder",
  "version": "0.0.1",
  "description": "React based client side one page website framework",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- -watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "static-assets": "babel-node tools/staticAssetManagement"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.9",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.18.0",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.11.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "expose-loader": "^0.7.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "globby": "^6.1.0",
    "jsdom": "9.8.3",
    "minimist": "^1.2.0",
    "mocha": "3.2.0",
    "nock": "9.0.2",
    "node-sass": "^3.13.0",
    "npm-run-all": "3.1.2",
    "open": "0.0.5",
    "react-addons-test-utils": "15.4.1",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.2.1",
    "rimraf": "2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justingorham/react-webpack-one-page-wonder.git"
  },
  "keywords": [],
  "author": "Justin Gorham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justingorham/react-webpack-one-page-wonder/issues"
  },
  "homepage": "https://github.com/justingorham/react-webpack-one-page-wonder#readme",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "url": "^0.11.0",
    "whatwg-fetch": "^2.0.1"
  }
}
